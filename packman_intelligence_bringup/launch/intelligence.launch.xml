<?xml version="1.0"?>

<launch>
<!--  <arg name="robot_prefix" default=""/>-->
  <arg name="ROS_HOME" default="$(env ROS_HOME $(env HOME)/.ros)"/>

  <set_parameter name="use_sim_time" value="true" />

  <include file="$(find-pkg-share nav2_bringup)/launch/bringup_launch.py">
    <arg name="map" value="$(var ROS_HOME)/maps/map.yaml"/>
    <arg name="params_file" value="$(find-pkg-share packman_intelligence_bringup)/config/nav2_params.yaml"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="autostart" value="true"/>
  </include>

</launch>
